---
import MainLayout from "./MainLayout.astro";
import { AuthGuard } from "../components/AuthGuard";

interface Props {
  title: string;
  description?: string;
}

const { title, description } = Astro.props;
---

<MainLayout title={title} description={description}>
  <AuthGuard client:load>
    <slot />
  </AuthGuard>
</MainLayout>
